{
  "$schema": "https://schemas.wp.org/wp/6.5/theme.json",
  "version": 2,
  "templateParts": [
    {
      "name": "header-no-site-title",
      "title": "Header No Site Title",
      "area": "header"
    },
    {
      "name": "header-site-title",
      "title": "Header Site Title",
      "area": "header"
    },
    {
      "title": "Footer Container",
      "name": "footer",
      "area": "footer"
    }
  ],
  "styles": {
    "spacing": {
      "padding": {
        "top": "0px",
        "right": "2rem",
        "bottom": "0px",
        "left": "2rem"
      }
    }
  },
  "settings": {
    "appearanceTools": true,
    "useRootPaddingAwareAlignments": true,
    "layout": {
      "contentSize": "1440px",
      "wideSize": "1440px"
    },
    "typography": {
      "fluid": true,
      "fontSizes": [
      {
        "slug": "h1",
        "size": "var(--dswp-typography-regular-H1)",
        "name": "Heading 1"
      },
      {
        "slug": "h2",
        "size": "var(--dswp-typography-regular-H2)",
        "name": "Heading 2"
      },
      {
        "slug": "h3",
        "size": "var(--dswp-typography-regular-H3)",
        "name": "Heading 3"
      },
      {
        "slug": "h4",
        "size": "var(--dswp-typography-regular-H4)",
        "name": "Heading 4"
      },
      {
        "slug": "h5",
        "size": "var(--dswp-typography-regular-H5)",
        "name": "Heading 5"
      }
    ]
    },
    "spacing": {
      "blockGap": null,
      "margin": true
    },
    "color": {
      "palette": [
        {
          "slug": "header-color-background",
          "color": "blue",
          "name": "Header background color"
        },
        {
          "slug": "menu-color-background",
          "color": "#ffffff",
          "name": "Menu background color"
        },
        {
          "slug": "typography-primary-color",
          "color": "#2d2d2d",
          "name": "Primary color"
        },
        {
          "slug": "foreground",
          "color": "var(--wp--preset--color--typography-primary-color)",
          "name": "Foreground"
        },
        {
          "slug": "background",
          "color": "#ffffff",
          "name": "Background"
        }
      ]
    }
  }
}